Text text={menuTitle},x=85,y=10,width=405,height=40,lines=1,align=center,red=0,green=0,blue=0;

// Senderlogo Logo oben links
Image x=5,y=5,width=60,height=30,fit=yes,aspect_ratio=yes,path=columnimages/{eventChannelName/tologo}.png,on_click=Menu;

//Start und Endzeit des Programms ausgeben
Text text={eventStartTime/%a %d.%m},x=10,y=90,size=20,width=170,height=30;
Text text={eventStartTime/%H:%M},x=10,y=130,size=20,width=80,height=30;
Text text=-{eventEndTime/%H:%M},x=90,y=130,size=20,width=90,height=30;

Rectangle x=185,y=70,width=3,height=115;

text text={eventTitle},x=195,y=80,width=350,height=40,red=0,green=0,blue=144;
text text={eventSubtitle},x=195,y=120,size=18,width=350,height=65,lines=2,red=0,green=0,blue=255;

//EPG Bild ausgeben
Image x=545,y=75,width=175,height=105,
	path=
	{varEPGimagesPath_1}{eventID}.png:
	{varEPGimagesPath_2}{eventID}.png:
	{varEPGimagesPath_3}{eventID}.png:
	{varImagesPath_1}{eventTitle}.jpg:
	menu/menuitems/logo.png,
	fit=yes,aspect_ratio=yes;

Rectangle x=5,y=185,width=710,height=3;

//Ausgabe Detailinformationen
Text text={eventDescription},start_line={varStartLine},
   x=10,y=200,width=700,height=270,size=20,red=0,green=0,blue=144,
   whipe_res=28,on_up=varStartLine--,on_down=varStartLine++;

#ifdef MOUSE_BUTTONS
	var varHideColorButtons = 1;
	var varHidePrevChannel = 1;
	Include=MouseButtons;
#endif

