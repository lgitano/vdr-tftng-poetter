//***************************************************************************
// Allgemeine Einstellungen für kommende Wiedergabe Abschnitte (Replay)
// Dieser Abschnitt wird in anderen Sektionen eingebunden (include)
//***************************************************************************
[ReplayCommon]

Defaults font=graphTFT,size=28,red=0,green=0,blue=144,bg_red=0,bg_green=0,bg_blue=150,bg_transparent=0;
Background path=backgrounds/movie_bg.png;

//Uhr oben rechts
Text text={time/%d.%m. %H:%M},x=380,y=7,width=250,height=35,lines=1,size=22,align=right,red=0,green=0,blue=0;

text condition={actRecordingCount} > 0,
   text={actRecordingName},
   x=220,y=45,width=490,height=100,lines=1,size=22,align=right,red=255,green=0,blue=0;

//Buttons 
// pause
Image condition={replayPlay} = 1,x=455,y=90,path=symbols/pause_off.png,delay=1;
Image condition={replayForward} = 1 & {replaySpeed} = -1 & {replayPlay} <  1,x=455,y=90,path=symbols/pause_on.png,delay=1;

// play
Image condition={replayPlay} < 1,x=375,y=90,path=symbols/play_off.png,delay=1;
Image condition={replayForward} = 1 & {replaySpeed} = -1 & {replayPlay} = 1,x=375,y=90,path=symbols/play_on.png,delay=1;

// Symbole für Vorspulen
// Slow Forward
Image condition={replaySpeed} = -1,x=535,y=90,path=symbols/sf_off.png,delay=1;
Image condition={replayForward} = 1 & {replaySpeed} > 0 & {replayPlay} <  1,x=535,y=90,path=symbols/sf_on.png,delay=1;
// Fast Forward
Image condition={replaySpeed} = -1,x=615,y=90,path=symbols/ff_off.png,delay=1;
Image condition={replayForward} = 1 & {replaySpeed} > 0 & {replayPlay} = 1,x=615,y=90,path=symbols/ff_on.png,delay=1;

// Symbole für Zurückspulen
// Slow Rewind
Image condition={replaySpeed} = -1,x=215,y=90,path=symbols/sb_off.png,delay=1;
Image condition={replayForward} = 0 & {replaySpeed} > 0 & {replayPlay} <  1,x=215,y=90,path=symbols/sb_on.png,delay=1;
// Fast Rewind
Image condition={replaySpeed} = -1,x=295,y=90,path=symbols/fb_off.png,delay=1;
Image condition={replayForward} = 0 & {replaySpeed} > 0 & {replayPlay} = 1,x=295,y=90,path=symbols/fb_on.png,delay=1;

//Multiplikatorausgabe für Geschwindigkeiten (2x - 9x)
//Image condition={replaySpeed} > 1,x=99,y=483,path=symbols/{replaySpeed}.png,delay=1;

//Laufzeiten links und rechts unten
Text text={replayCurrent/%k:%M:%S},x=30,y=455,width=200,height=40,delay=1,size=30,lines=1,red=0,green=0,blue=144;
Text text={replayTotal/%k:%M:%S},x=490,y=455,width=200,height=40,align=right,delay=1,size=30,lines=1,red=0,green=0,blue=144;

//Fortschrittsanzeige
Progressbar total={replayTotal/%s},value={replayCurrent/%s},x=33,y=500,width=657,height=30,bg_x=30,bg_y=497,bg_width=660,bg_height=36,path=symbols/progress.png,bg_transparent=255,delay=3;

Include=Volume;
Include=OSD-Messages;



